{
  "compilerOptions": {
    "target": "ES2019",
    "module": "commonjs",
    "sourceMap": false,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ]
  },
  "files": [
    "estree.ts"
  ],
  "exclude": [
    "node_modules",
    "tests"
  ]
}